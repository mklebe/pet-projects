{
  "routes": [
    {
        "src": "/(.*)",
        "dest": "dist/packages/webclient",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
    },
    {
      "src": "/api/(.*)",
      "dest": "dist/packages/api/main.js",
      "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
      ]
  }
  ]
}